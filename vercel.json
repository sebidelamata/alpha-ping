{
    "headers": [
      {
        "source": "/(.*)",
        "headers": [
          {
            "key": "Content-Security-Policy",
            "value": "default-src 'self'; script-src 'self' 'unsafe-inline' https://*.walletconnect.org; frame-ancestors 'self' https://secure.walletconnect.org;"
          }
        ]
      }
    ]
  }
  